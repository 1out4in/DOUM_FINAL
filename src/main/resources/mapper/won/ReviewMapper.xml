<mapper namespace="com.example.doum.mapper.won.ReviewMapper">

    <insert id="insertReview">
        insert into tbl_organization_review
        values (seq_organization_review.nextval, #{organizationId}, #{userId}, #{content}, #{rating})
    </insert>


</mapper>